{
  "coc.preferences.formatOnSaveFiletypes": [],
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "eslint.format.enable": true,
  "languageserver": {
    "ocaml-lsp": {
      "command": "opam",
      "args": ["exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    },
    "sql": {
      "command": "sql-language-server",
      "args": [
        "up",
        "--method",
        "stdio"
      ],
      "filetypes": [
        "sql",
        "mysql"
      ]
    },
    "wgsl": {
      "command": "wgsl_analyzer",
      "filetypes": ["wgsl"]
    }
  },
  "python.autoComplete.extraPaths": [
    "/Applications/Talon.app/Contents/Resources/python/lib/python3.9/site-packages/talon/"
  ],
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.cargo.target": null,
  "rust-analyzer.debug.enable": true,
  "rust-analyzer.debug.runtime": "vimspector",
  "rust-analyzer.debug.vimspector.configuration.name": "Rust - lldb",
  "rust-analyzer.diagnostics.enable": true,
  "rust-analyzer.inlayHints.chainingHints": true,
  "rust-analyzer.inlayHints.typeHints": true,
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.lens.implementations.enable": true,
  "rust-analyzer.lens.run": true,
  "solargraph.autoformat": true,
  "solargraph.formatting": true,
  "solargraph.diagnostics": true,
  "solargraph.hover": true
}
